import java.util.*;
public class Exercicio_3_Matriz {
public static void main (String [] args) {
	Random leia = new Random (70);
	int lin, col;
	int m[][]  = new int [5][5];
	for(lin = 0; lin<4; lin++) {
		for(col=0; col<4; col++) {
			m[lin][col] = leia.nextInt();
			
		}
	}

	for(lin = 0; lin<4; lin++) {
		for(col=0; col<4; col++) {
		}
	
		}
		
}

}

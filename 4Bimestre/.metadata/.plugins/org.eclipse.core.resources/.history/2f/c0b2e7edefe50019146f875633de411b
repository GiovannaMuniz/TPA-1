import java.util.*;
public class Exercicio_8_Somatorio {
public static void main (String [] args) {
	Scanner leia = new Scanner (System.in);
	int a[] =  new int [10];
	int b[] = new int [10];
	int i, j, soma = 0;
	
	for(i = 0; i<10; i++) {
		System.out.println("Escreva o numero da "+(i+1)+" posição");
		a[i] = leia.nextInt();
	}
	
	System.out.print("B{");
	for(j =0, i= 0; j<10; i++,j++) {
		soma = soma+a[i];
		b[j] = soma;
		System.out.println(b[j]+" ");
	}
	System.out.println("}");
}
}

import java.util.*;
public class Exercicio_3_Primo {
public static void main (String [] args) {
	Scanner leia = new Scanner (System.in);
	int i,r, a[] = new int [5];
	for (i = 0; i<5; i++ ) {
		System.out.println("Coloque o numero");
		a[i] = leia.nextInt();
		for(int j=1, p=0; j<a[i];j++) {
			r = a[i]%j;
			System.out.println(p);
			if (r==0) {
				p++;
			}
			
			}
		
		if(p<=2) {
			System.out.println("Seu numero é Primo");
		}else {
			
			System.out.println("Seu numero não é Primo");
		}
	}
	
}
}

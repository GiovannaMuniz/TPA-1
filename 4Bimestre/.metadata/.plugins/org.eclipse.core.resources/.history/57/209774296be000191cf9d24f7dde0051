import java.util.*;
public class Exercicio_15_Crescente_Descrescentes {
public static void main (String [] args ) {
	Scanner leia = new Scanner (System.in);
	int A[] = new int [5];
	int B[] = new int [5];
	int C[] = new int [5];
	int  aux = 0, ns, i, f;
	for (ns = 0; ns<5; ns++) {
		System.out.printf("Fale o %d numero\n",ns+1);
		A[ns] = leia.nextInt();
	
	}
	for (f = 5; f>0; f--) {
		if(A[f] > A[f+1]) {
			aux=A[f];
			A[f]=A[f+1];
			A[f+1]=aux;
			System.out.print(A[f]+" ");
			
		}
		System.out.println(" ");
		
		
	}
	
}
}

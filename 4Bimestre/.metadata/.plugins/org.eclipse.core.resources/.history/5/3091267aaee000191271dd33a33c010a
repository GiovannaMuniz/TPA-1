import java.util.*;
public class Exercicio_5_Divisiveis {
public static void main (String [] args) {
	Scanner leia = new Scanner (System.in);
	int a[] = new int [10];
	int i, t, r;
	for(i=0, t=1; i<10; i++) {
		System.out.println("Coloque o numero");
		a[i] = leia.nextInt();
		r = a[i]%t;
		System.out.printf("%d = {",a[i]);
		if(r == 0) {
			System.out.printf("%d ",t);
		}
		t++;
		System.out.printf("}\n");
}
}
}